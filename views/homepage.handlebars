{{#each posts as |post|}}
  <div>
    <div class="postBox">
      <div class="postTitle"><h3>{{post.title}}</h1><h9>Posted by
          {{post.user.username}}
          on
          {{format_date createdAt}}</h9></div>
      <div><p>{{post.body}}</p></div>
    </div>
  </div>
{{/each}}

<script src="/js/homepage.js"></script>
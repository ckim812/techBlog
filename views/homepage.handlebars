{{#each posts as |post|}}
  <div>
    <div class="postBox">
      <div class="postTitle"><a
          href="/createnewcomment/{{post.id}}"
        ><h3>{{post.title}}</h3></a><h9>Posted by
          {{post.user.username}}
          on
          {{format_date post.createdAt}}</h9></div>
      <div><p>{{post.body}}</p></div>
    </div>
  </div>
{{/each}}